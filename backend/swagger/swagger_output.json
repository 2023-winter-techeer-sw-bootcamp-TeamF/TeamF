  "paths": {
    "/tarot/guide": {
      "get": {
        "tags": [
          "Tarot"
        ],
        "summary": "가이드라인 불러오기",
        "description": "운 종류와 뽑는 사람 수를 전달하면 가이드라인(content)과 타로 마스터 이름(master_name)을 반환함",
        "parameters": [
          {
            "name": "luckType",
            "in": "query",
            "description": "운 종류",
            "required": true,
            "type": "string",
            "example": "오늘의 운세, 애정운, 우정운, 재물운, 소망운",
            "value": "test_luck"
          },
          {
            "name": "luckOpt",
            "in": "query",
            "description": "카드를 뽑는 사람의 수",
            "required": true,
            "type": "integer",
            "example": "0(혼자 보는 경우), 1(같이 보는 경우)",
            "value": "0"
          }
        ],
        "responses": {
          "200": {
            "description": "테스트 값 조회 성공",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "가이드라인과 타로 마스터 이름 불러오기 성공"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "master_name": {
                      "type": "string",
                      "example": "test_master_name"
                    },
                    "content": {
                      "type": "string",
                      "example": "test_content"
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "잘못된 요청",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "운 종류나 뽑는 사람 수가 없습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "DB 조회 과정에서 오류 발생 시의 응답",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "DB 조회 오류"
                },
                "error": {
                  "type": "string",
                  "example": "운 카테고리 Table에서 데이터 조회 중 오류 발생"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/tarot/card/info": {
      "post": {
        "tags": [
          "Tarot"
        ],
        "summary": "카드 정보 조회",
        "description": "카드 정보를 정수형으로 전달하면 해당 카드의 정보를 반환함",
        "parameters": [
          {
            "name": "card",
            "in": "query",
            "description": "카드 번호",
            "required": true,
            "type": "integer",
            "example": "1",
            "schema": {
              "type": "object",
              "properties": {
                "card": {
                  "type": "number",
                  "example": 1
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "카드 정보 불러오기 성공 url 전송",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "creat image url successfully"
                },
                "name": {
                  "type": "string",
                  "example": "카드 이름"
                },
                "english": {
                  "type": "string",
                  "example": "카드 영어 이름"
                },
                "mean": {
                  "type": "string",
                  "example": "카드 뜻"
                },
                "image_url": {
                  "type": "string",
                  "example": "카드 이미지 url"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "잘못된 카드 번호 요청",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "카드 번호가 없습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "S3 관련 오류 발생 시의 응답",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "카드 정보를 가져오는데 실패했습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
    "/poll/create": {
      "get": {
        "tags": [
          "Tarot"
        ],
        "summary": "타로 시작 시 Poll(임시저장) → 타로 시작 할 경우 뽑은 카드와 결과 저장을 구별할 Poll Table",
        "description": "뽑은 카드 결과 저장 및 총 결과 저장 시 사용됨",
        "responses": {
          "200": {
            "description": "Poll ID가 성공적으로 생성되었을 때의 응답",
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "string",
                  "example": "success"
                },
                "statusCode": {
                  "type": "number",
                  "example": 200
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Poll ID 생성 완료"
                    },
                    "pollId": {
                      "type": "number",
                      "example": 0
                    }
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "401": {
            "description": "Unauthorized: 엑세스 토큰을 복호화한 정보(user_id)가 없을 시의 응답",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "엑세스 토큰이 없습니다."
                },
                "error": {
                  "type": "string",
                  "example": "엑세스 토큰이 필요합니다"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "DB 저장 과정에서 오류 발생 시의 응답",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "DB 저장 오류"
                },
                "error": {
                  "type": "string",
                  "example": "타로 결과(poll) Table에 데이터 저장 중 오류 발생"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        },
        "security": [
          {
            "Bearer": []
          }
        ]
      }
    },
    "/result/save": {
      "post": {
        "tags": [
          "Result"
        ],
        "summary": "종합 결과 저장",
        "description": "사용자의 질문과 관련된 타로 종합 결과 및 뽑은 카드 정보를 저장합니다. 사용자의 운(luck)에 따라 카드의 수가 결정됩니다.",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "description": "타로 결과 및 뽑은 카드 정보 저장을 위한 요청값",
            "required": true,
            "schema": {
              "type": "object",
              "properties": {
                "poll_id": {
                  "type": "string",
                  "example": "1"
                },
                "question": {
                  "type": "string",
                  "example": "사용자 질문"
                },
                "result_explanation": {
                  "type": "string",
                  "example": "종합 결과"
                },
                "master_name": {
                  "type": "string",
                  "example": "마스터 이름"
                },
                "luck": {
                  "type": "string",
                  "example": "test_luck"
                },
                "cards": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "card_image_url": {
                        "type": "string",
                        "example": "url3"
                      },
                      "card_explanation": {
                        "type": "string",
                        "example": "explanation3"
                      },
                      "card_eng_name": {
                        "type": "string",
                        "example": "eng3"
                      },
                      "card_kor_name": {
                        "type": "string",
                        "example": "kor3"
                      }
                    }
                  }
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "타로 종합 결과, 뽑은 카드별 정보 저장 성공",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "타로 종합 결과, 뽑은 카드별 정보 저장 성공"
                },
                "resultId": {
                  "type": "number",
                  "example": 26
                },
                "cardIds": {
                  "type": "array",
                  "example": [
                    1,
                    2,
                    3
                  ],
                  "items": {
                    "type": "number"
                  }
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "잘못된 요청",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "데이터가 유효하지 않습니다. (널값, 누락 등)"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "401": {
            "description": "Unauthorized"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "내부 서버 오류로 인한 데이터 처리 실패.",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "DB 오류"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        },
        "security": [
          {
            "Bearer": []
          }
        ]
      }
    },
