    "/tarot/card/info": {
      "post": {
        "tags": [
          "Tarot"
        ],
        "summary": "카드 정보 조회",
        "description": "카드 정보를 정수형으로 전달하면 해당 카드의 정보를 반환함",
        "parameters": [
          {
            "name": "card",
            "in": "query",
            "description": "카드 번호",
            "required": true,
            "type": "integer",
            "schema": {
              "type": "object",
              "properties": {
                "card": {
                  "type": "number",
                  "example": 1
                }
              },
              "required": [
                "card"
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "카드 정보 불러오기 성공, url 전송",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "creat image url successfully"
                },
                "name": {
                  "type": "string",
                  "example": "카드 이름"
                },
                "english": {
                  "type": "string",
                  "example": "영어 이름"
                },
                "mean": {
                  "type": "string",
                  "example": "해석"
                },
                "image_url": {
                  "type": "string",
                  "example": "이미지 url"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "400": {
            "description": "잘못된 카드 번호 요청",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "잘못된 카드 번호 요청"
                }
              },
              "xml": {
                "name": "main"
              }
            }
          },
          "500": {
            "description": "카드 정보 불러오기 실패",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string",
                  "example": "카드 정보를 가져오는데 실패했습니다."
                }
              },
              "xml": {
                "name": "main"
              }
            }
          }
        }
      }
    },
